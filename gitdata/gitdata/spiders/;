# -*- coding: utf-8 -*-
import scrapy


class GitspiderSpider(scrapy.Spider):
    name = 'gitspider'
    url_tmp = 'https://github.com/shiyanlou?page={}&tab=repositories'

    @property
    def start_urls(self):
        return (url_tmp.format(i) for i in range(1:5))

    def parse(self, response):
        pass
